    %vpi_call/w 3 143 "$display", "------------------------------------" {0 0 0};
    %vpi_call/w 3 145 "$display", "===================================" {0 0 0};
    %vpi_call/w 3 146 "$display", "Register File Testbench Finished." {0 0 0};
    %vpi_call/w 3 147 "$finish" {0 0 0};
    %end;
    .thread T_2;
